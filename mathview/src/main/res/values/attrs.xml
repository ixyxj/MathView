<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MathWebView">
        <attr name="text" format="string"/>
        <attr name="config_type" format="enum">
            <enum name="TeX" value="0"/>
            <enum name="MML" value="1"/>
            <enum name="AM" value="2"/>
            <enum name="TXT" value="3"/>
        </attr>
        <attr name="engine" format="enum">
